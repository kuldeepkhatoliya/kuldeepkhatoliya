
#include <Servo.h>

Servo s1; 
Servo s2; 
Servo s3; 

Servo s4; 
Servo s5; 

void setup() {
  s1.attach(5); 
  s2.attach(6); 
  s3.attach(9); 

  s4.attach(10); 
  s5.attach(11); 
 
}

void loop() {
  
  s1.write(0);
  s2.write(60);
  s3.write(60); 
  
  s4.write(0);

 

  
delay(1000);
s1.write(60);
s2.write(0);
s3.write(0);
s4.write(60);
delay(1000);

s5.write(0);
delay(500);
s5.write(60);
delay(500);
s5.write(0);
delay(500);
s5.write(60);
delay(1000);
s5.write(30);
delay(500);


s1.write(0);
  s2.write(60);
  s3.write(60); 
  
  s4.write(0);

 

  
delay(1000);
s1.write(60);
s2.write(0);
s3.write(0);
s4.write(60);
delay(1000);
s5.write(0);
delay(500);
s5.write(60);
delay(500);
s5.write(0);
delay(500);
s5.write(60);
delay(1000);
s5.write(30);

delay(500);
s2.write(60);
s4.write(0);
delay(500);
s2.write(0);
s4.write(60);
delay(1000);


}
